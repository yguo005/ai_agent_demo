# Copy this file to .env and add your actual API keys

# OpenAI API Key (for embeddings and AI analysis)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (alternative to OpenAI - optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Redis Connection (optional - will use in-memory fallback if not provided)
REDIS_URL=redis://localhost:6379

# HoneyHive API Key (for observability and MCP compliance)
HONEYHIVE_API_KEY=your_honeyhive_api_key_here

# Environment
ENVIRONMENT=development
